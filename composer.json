{
  "name": "powerkernel/yii2-support",
  "description": "Yii2 Support Ticket System",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "support",
    "ticket"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Harry Tang",
      "email": "harry@powerkernel.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "~2.0",
    "hashids/hashids": "^2.0",
    "yiidoc/yii2-redactor": "*",
    "php-imap/php-imap": "^3.0",
    "yiisoft/yii2-queue": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "powerkernel\\support\\": ""
    }
  },
  "extra": {
    "bootstrap": "powerkernel\\support\\Bootstrap"
  }
}
